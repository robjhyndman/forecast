  nnet,
  parallel,
  Rcpp (>= 0.11.0),
  stats,
  timeDate,
  tseries,
  urca,
  uroot,
  zoo
Suggests:
  expsmooth,
  knitr,
  rmarkdown,
  rticles,
  testthat
LinkingTo:
  Rcpp (>= 0.11.0),
  RcppArmadillo (>= 0.2.35)
LazyData: yes
ByteCompile: TRUE
